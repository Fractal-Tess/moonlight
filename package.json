{
  "name": "starlight-theme-moonlight-monorepo",
  "version": "0.2.1",
  "license": "MIT",
  "description": "Starlight theme inspired by the Visual Studio Code Vitesse theme.",
  "author": "HiDeoo <github@hideoo.dev> (https://hideoo.dev)",
  "type": "module",
  "prettier": "@fractal-tess/prettier-config",
  "scripts": {
    "lint": "astro check --noSync && pnpm -r lint",
    "format": "prettier -w --cache --ignore-unknown .",
    "version": "changeset version && pnpm i --no-frozen-lockfile",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@fractal-tess/prettier-config": "^0.1.2",
    "@hideoo/eslint-config": "^4.0.0",
    "@hideoo/prettier-config": "^2.0.0",
    "@types/node": "^24.5.2",
    "astro": "^5.14.0",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.17.1",
  "private": true,
  "sideEffects": false,
  "keywords": [
    "starlight",
    "theme",
    "moonlight",
    "vitesse",
    "plugin",
    "documentation",
    "astro"
  ],
  "homepage": "https://github.com/HiDeoo/starlight-theme-moonlight",
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/starlight-theme-moonlight.git"
  },
  "bugs": "https://github.com/HiDeoo/starlight-theme-moonlight/issues"
